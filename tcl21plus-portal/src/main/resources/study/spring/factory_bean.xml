<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="userFactoryBean" class="com.movit.study.spring.factory_bean.UserFactoryBean">
    </bean>

    <bean id="getUserBean" class="com.movit.study.spring.factory_bean.GetUserBean">
        <property name="user" ref="userFactoryBean"></property>
    </bean>

</beans>